<body>
    <h1>TEXAS Rock Climbs</h1>
    <!-- Display climbs -->
    <% if (climbs.length > 0) { %>
        <% climbs.forEach(climb => { %>
            <div class="climb">
                <h3><%= climb.name %></h3>
                <p><strong>Location:</strong> <%= climb.location %></p>
                <p><strong>Difficulty:</strong> <%= climb.difficulty %></p>
                <p><strong>Description:</strong> <%= climb.description %></p>
            </div>
        <% }); %>
    <% } else { %>
        <p>No climbs available.</p>
    <% } %>


</body>
